package looptest;

/**
 *
 * @author elacy
 */
public class LoopTest {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        int counter = 0;
        
        do{
           System.out.println(counter);
           counter++;
        }while(counter <=10);
            
        }
       
    }

