package looptest;

/**
 *
 * @author elacy
 */
public class LoopTest {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        int counter = 0;
        while(counter <= 10){
            System.out.println(counter);
        counter++;
            
        }
        
    }
}
