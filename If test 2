package iftesting;

/**
 *
 * @author elacy
 */
public class Iftesting {

    public static void main(String[] args) {
       int boy = 18;
       int girl = 28;
      //%% can mean both
       // || is or
       if (boy >= 16 || girl <= 30){
           System.out.println("You can enter the club!");
       }
       else {
           System.out.println("You can not enter the club!");
       
    }
    }}
